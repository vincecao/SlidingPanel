{
  "name": "my-chrome-extension",
  "version": "0.1.0",
  "license": "MIT",
  "main": "index.tsx",
  "scripts": {
    "start": "parcel index.html",
    "build": "npm run style && parcel build index.html --public-url ./",
    "style": "npx tailwindcss -i ./src/input.css -o ./dist/output.css",
    "style:watch": "npx tailwindcss -i ./src/input.css -o ./dist/output.css --watch"
  },
  "dependencies": {
    "@blueprintjs/core": "^4.9.4",
    "classnames": "^2.3.1",
    "react": "^18.2.0",
    "react-app-polyfill": "^3.0.0",
    "react-dom": "^18.2.0",
    "react-frame-component": "^5.2.3"
  },
  "devDependencies": {
    "@types/react": "^18.0.19",
    "@types/react-dom": "^18.0.6",
    "parcel": "1.12.3",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.3"
  }
}
